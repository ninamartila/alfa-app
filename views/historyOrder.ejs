<!DOCTYPE html>
<html lang="en">
  <%- include('./patrials/header.ejs', {title: 'History Order' }); %>
  <body style="padding: 20px">
    <h1>History Order'</h1>
    <%- include('./patrials/navbar.ejs', {activeProduk: '' , activeUser:
    '' , activeOrder:'active', activeKeranjang: ''});%>
    <table class="table">
      <table class="table">
      <thead>
        <tr>
          <th scope="col">Image</th>
          <th scope="col">Nama Produk</th>
          <th scope="col">Harga Produk</th>
          <th scope="col">Kode Produk</th>
          <th scope="col">Total Qantity Produk</th>
          <th scope="col">Total Harga Produk</th>
        </tr>
      </thead>
      <tbody>
        <% dataProduk.forEach(item =>{ %>
        <tr>
          <td>
            <img src="<%= item.image %>" style="width: 50px" />
          </td>
          <td><%= item.nama_produk %></td>
          <td><%= item.harga_produk %></td>
          <td><%= item.kode_produk %></td>
          <td><%= item.quantity %></td>
          <td><%= item.total_harga %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
